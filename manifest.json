{
  "name": "TabNeT",
  "version": "5.0",
  "description": "Handy Shortcuts",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "tabGroups"],
  "commands": {
    "toggle-pin": {
      "suggested_key": { "default": "Ctrl+Shift+P" },
      "description": "Pin the current tab"
    }
  },
  "manifest_version": 3
}
